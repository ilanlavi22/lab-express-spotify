{{>search}}
<div class='feature'>
  <h3>Album Results</h3>
  <div class='featured-container'>
    {{#each album}}
      <div class='featured-item'>
        <div class='featured-img'>
          <a href='/tracks/{{id}}'>
            <img src='{{images.0.url}}' alt='album for {{name}}' />
          </a>
        </div>
        <div class='featured-name'><a href='/tracks/{{id}}'>{{name}}</a></div>
      </div>
    {{/each}}
  </div>
</div>

